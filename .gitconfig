[user]
    name = Austin Kim
    email = kim.austin.h@gmail.com
[core]
    excludesfile = ~/.gitignore_global
[difftool "sourcetree"]
    cmd = opendiff \"$LOCAL\" \"$REMOTE\"
    path = 
[mergetool "sourcetree"]
    cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
    trustExitCode = true
[color]
    diff = true
[alias]
    a = add
    s = status
    br = branch
    c = commit
    co = checkout
    d = diff
    ds = diff --stat
    h = help
    f = fetch
    m = merge
    rst = reset
    l = log
    rev = revert
    rv = remote -v
[push]
	default = simple
